Vagrant.configure(2) do |config|
  config.vm.box = "fedora-VAGRANTSLASH-25-cloud-base"
  config.vm.define :smartbox do |smartbox|
  end
  config.vm.provision "shell", inline: <<-SHELL
    sudo dnf install -y python yum libselinux-python
  SHELL
  config.vm.provision "ansible" do |ansible|
    ansible.playbook = "resources/playbook.yml"
    ansible.sudo = true
  end
end
